  address = {New York, NY, USA},
  doi = {10.1145/1138450.1138458},
  file = {:E\:\\personal\\maestria\\tesis\\Jeronimo\\trunk\\documents\\thesis\\TQLBO-paper\\papers\\Vanraes2006\\p340-vanraes.pdf:PDF},
  issn = {0730-0301},
  issue_date = {April 2006},
  keywords = {Powell-Sabin split, Subdivision, arbitrary topology},
  numpages = {16},
  owner = {apinzonf},
  publisher = {ACM},
  timestamp = {2012.09.11},
  url = {http://doi.acm.org/10.1145/1138450.1138458}
}

@ARTICLE{Wang2012,
  author = {Jun Wang and Xi Zhang and Zeyun Yu},
  title = {A cascaded approach for feature-preserving surface mesh denoising},
  journal = {Computer-Aided Design},
  year = {2012},
  volume = {44},
  pages = {597 - 610},
  number = {7},
  abstract = {Mesh denoising is crucial for improving noisy meshes acquired from
	scanning devices and digitization processes. This paper proposes
	a general, robust approach for mesh denoising by using a combination
	of bilateral filtering, feature recognition, anisotropic neighborhood
	searching, and surface fitting and projection techniques. Motivated
	by the bilateral filtering from image processing applications, we
	develop a new bilateral filter operating on the normal vector fields
	of the mesh. Then, we detect mesh features and classify mesh vertices
	into non-feature vertices and feature vertices. The corresponding
	anisotropic neighborhoods for each vertex are searched by constructing
	a weighted dual graph, over which biquadratic Bezier surface patches
	are fitted and projected. The projection points are averaged to update
	each vertex of the mesh. The steps above are repeated iteratively
	until convergence, i.e., the Hausdorff distance between two sequential
	denoised meshes is less than a pre-defined threshold. A number of
	examples presented in the paper demonstrate that our method generally
	yields visually and numerically better denoising results, compared
	with the state-of-the-art methods.},
  doi = {10.1016/j.cad.2012.03.001},
  file = {:E\:\\personal\\maestria\\tesis\\Jeronimo\\trunk\\documents\\thesis\\TQLBO-paper\\papers\\Wang2012\\wang2012.pdf:PDF},
  issn = {0010-4485},
  keywords = {Mesh denoising},
  owner = {apinzonf},
  timestamp = {2012.09.12},
  url = {http://www.sciencedirect.com/science/article/pii/S0010448512000516}
}

@INPROCEEDINGS{Wang2006,
  author = {Wang, Ke and Weiwei and Tong, Yiying and Desbrun, Mathieu and Schr\"{o}der,
	Peter},
  title = {Edge subdivision schemes and the construction of smooth vector fields},
  booktitle = {ACM SIGGRAPH 2006 Papers},
  year = {2006},
  series = {SIGGRAPH '06},
  pages = {1041--1048},
  address = {New York, NY, USA},
  publisher = {ACM},
  abstract = {Vertex- and face-based subdivision schemes are now routinely used
	in geometric modeling and computational science, and their primal/dual
	relationships are well studied. In this paper, we interpret these
	schemes as defining bases for discrete differential 0- resp. 2-forms,
	and complete the picture by introducing edge-based subdivision schemes
	to construct the missing bases for discrete differential 1-forms.
	Such subdivision schemes map scalar coefficients on edges from the
	coarse to the refined mesh and are intrinsic to the surface. Our
	construction is based on treating vertex-, edge-, and face-based
	subdivision schemes as a joint triple and enforcing that subdivision
	commutes with the topological exterior derivative. We demonstrate
	our construction for the case of arbitrary topology triangle meshes.
	Using Loop's scheme for 0-forms and generalized half-box splines
	for 2-forms results in a unique generalized spline scheme for 1-forms,
	easily incorporated into standard subdivision surface codes. We also
	provide corresponding boundary stencils. Once a metric is supplied,
	the scalar 1-form coefficients define a smooth tangent vector field
	on the underlying subdivision surface. Design of tangent vector fields
	is made particularly easy with this machinery as we demonstrate.},
  acmid = {1141991},
  doi = {10.1145/1179352.1141991},
